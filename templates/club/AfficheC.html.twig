<h1>Clubs <a href="club_add"><Button title="Add club">Add</Button></a></h1>
<table>
    <tr>
        <th>
            REF
        </th>
        <th>
            Creation date
        </th>
    </tr>
    {% for club in clubs %}
        <tr>
            <td>
                {{ club.ref }}
            </td>
            <td>
                {{ club.creationDate }}
            </td>
            <td>
                <a href="/club_update/{{ club.ref }}"><button>Update</button></a>
                <a href="/club_delete/{{ club.ref }}"><button>Delete</button></a>

            </td>
        </tr>
    {% endfor %}
</table>
